{
  "root": true,
  "extends": ["trybe-fundamentals"],
  "rules": {
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "prefer-const": "off",
    "prefer-destructuring": ["error", {"object": true, "array": false}], //remover depois
    "no-restricted-syntax": ["off", "FunctionExpression", "WithStatement", "BinaryExpression[operator='in']"]
  },
  "env": {
    "browser": false,
    "node": true,
    "jest": true
  }
}